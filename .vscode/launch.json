{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug IllogicApps.Test",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/IllogicApps.Test/bin/Debug/net8.0/IllogicApps.Test.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureLogicAppsStandard.pickProcess}"
        }
    ]
}
