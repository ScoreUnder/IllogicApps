{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "generateDebugSymbols",
			"command": "dotnet",
			"args": [
				"${input:getDebugSymbolDll}"
			],
			"type": "process",
			"problemMatcher": "$msCompile"
		},
		{
			"type": "shell",
			"command": "${config:azureLogicAppsStandard.funcCoreToolsBinaryPath}",
			"args": [
				"host",
				"start"
			],
			"problemMatcher": "$func-watch",
			"isBackground": true,
			"label": "func: host start",
			"group": {
				"kind": "build",
				"isDefault": false
			}
		},
		{
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"IllogicApps.Test/IllogicApps.Test.fsproj"
			],
			"problemMatcher": "$msCompile",
			"isBackground": false,
			"label": "Run IllogicApps.Test",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	],
	"inputs": [
		{
			"id": "getDebugSymbolDll",
			"type": "command",
			"command": "azureLogicAppsStandard.getDebugSymbolDll"
		}
	]
}
